<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-8 col-md-offset-2">
      <%= render 'shared/sources-menu' %>
      <h5>TECHCRUNCH - LATEST</h5>
      <div class="text-center">
      <% @techcrunches.each do |t| %>
        <%= link_to "#{t.url}" do %>
          <div class="card-article">
            <h3><%= t.title %></h3>
            <p><%= t.excerpt %></p>
    <!--         <p><%= t.url %></p> -->
          </div>
        <% end %>
      <% end %>
      </div>
    </div>
  </div>
</div>
