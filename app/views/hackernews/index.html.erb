<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-8 col-md-offset-2">
      <%= render 'shared/sources-menu' %>
        <div id="logo-techmeme">
          <h5>HACKERNEWS - Latest Posts</h5>
        </div>
      <div class="text-center">
      <% @hackernews.each do |h| %>
        <div class="card-article">
          <h3><%= h.title %></h3>
          <p><%= h.source %></p>
        </div>
      <% end %>
      <%= paginate @hackernews %>
      </div>
    </div>
  </div>
</div>
